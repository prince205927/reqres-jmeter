            <stringProp name="parameters"></stringProp>
            <stringProp name="script">
import groovy.json.JsonSlurper

//parse response json
def response = prev.getResponseDataAsString()
def json = new JsonSlurper().parseText(response)

//validating the presence of keys
def expectedKeys = [&apos;name&apos;,&apos;job&apos;,&apos;id&apos;,&apos;createdAt&apos;]
expectedKeys.each { k -&gt;
    assert json.containsKey(k) : &quot;Missing key &apos;${k}&apos; in POST /users response&quot;
}

//basic key types check
def isString = { v -&gt; v instanceof CharSequence }

assert isString(json.name)      : &quot;name must be string&quot;
assert isString(json.job)       : &quot;job must be string&quot;
assert isString(json.id)        : &quot;id must be string&quot;
assert isString(json.createdAt) : &quot;createdAt must be string&quot;

//matching against csv provided values
def expectedName = vars.get(&apos;name&apos;)
def expectedJob  = vars.get(&apos;job&apos;)
if (expectedName) assert json.name == expectedName :
    &quot;name mismatch: got &apos;${json.name}&apos;, expected &apos;${expectedName}&apos;&quot;
if (expectedJob)  assert json.job  == expectedJob  :
    &quot;job mismatch: got &apos;${json.job}&apos;, expected &apos;${expectedJob}&apos;&quot;

// --- Status code vs CSV expectedStatusCode (default 201) ---
def expected = (vars.get(&apos;expectedStatusCode&apos;) ?: &apos;201&apos;) as int
def actual   = (prev.getResponseCode()) as int
assert actual == expected :
    &quot;Expected ${expected}, got ${actual} (TC=${vars.get(&apos;testCaseId&apos;)}, remarks=${vars.get(&apos;remarks&apos;)})&quot;
</stringProp>
