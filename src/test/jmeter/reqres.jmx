          <stringProp name="script">import org.openqa.selenium.By
import org.openqa.selenium.WebElement
import org.openqa.selenium.chrome.ChromeDriver
import org.openqa.selenium.chrome.ChromeOptions
import org.openqa.selenium.support.ui.WebDriverWait
import org.openqa.selenium.support.ui.ExpectedConditions as EC
import java.time.Duration

final String BASE_URL        = &apos;https://app.reqres.in/&apos;
final String DASHBOARD_USER  = &apos;kopobe4558@bialode.com&apos;
final String DASHBOARD_PASS  = &apos;nVaAxcnogJraj98xxWE6&apos;
final Duration EXPL_WAIT     = Duration.ofSeconds(20)
final Duration IMPL_WAIT     = Duration.ofSeconds(2)
final Duration PAGE_LOAD_MAX = Duration.ofSeconds(30)

ChromeOptions options = new ChromeOptions()
options.addArguments(&apos;--headless=new&apos;)
options.addArguments(&apos;--disable-gpu&apos;)
options.addArguments(&apos;--window-size=1920,1080&apos;)
options.addArguments(&apos;--no-sandbox&apos;)
options.addArguments(&apos;--disable-dev-shm-usage&apos;)

ChromeDriver driver = new ChromeDriver(options)
driver.manage().timeouts().implicitlyWait(IMPL_WAIT)
driver.manage().timeouts().pageLoadTimeout(PAGE_LOAD_MAX)
WebDriverWait wait = new WebDriverWait(driver, EXPL_WAIT)

def logInfo = { msg -&gt; log.info(&apos;[Selenium] &apos; + msg) }
def logWarn = { msg -&gt; log.warn(&apos;[Selenium] &apos; + msg) }

def clickWhenClickable = { By locator -&gt;
    WebElement el = wait.until(EC.elementToBeClickable(locator))
    el.click()
}

def typeWhenVisible = { By locator, String text -&gt;
    WebElement el = wait.until(EC.visibilityOfElementLocated(locator))
    el.clear()
    el.sendKeys(text)
}

def getVisibleText = { By locator -&gt;
    WebElement el = wait.until(EC.visibilityOfElementLocated(locator))
    String t = el.getText()
    if (t == null || t.trim().isEmpty()) {
        t = el.getAttribute(&apos;textContent&apos;) ?: &apos;&apos;
    }
    return t.trim()
}

try {
    logInfo(&apos;Opening dashboard: &apos; + BASE_URL)
    driver.get(BASE_URL)

    logInfo(&apos;Clicking Sign In button&apos;)
    clickWhenClickable(By.cssSelector(&quot;button[component=&apos;SignInButton&apos;]&quot;))

    logInfo(&apos;Entering email and continuing&apos;)
    typeWhenVisible(By.id(&quot;identifier-field&quot;), DASHBOARD_USER)
    clickWhenClickable(By.xpath(&quot;//button[normalize-space()=&apos;Continue&apos;]&quot;))

    logInfo(&apos;Entering password and continuing&apos;)
    typeWhenVisible(By.id(&quot;password-field&quot;), DASHBOARD_PASS)
    clickWhenClickable(By.xpath(&quot;//button[normalize-space()=&apos;Continue&apos;]&quot;))

    logInfo(&apos;Navigating to API Keys&apos;)
    clickWhenClickable(By.linkText(&quot;API Keys&quot;))

    logInfo(&apos;Revealing API Key&apos;)
    clickWhenClickable(By.cssSelector(&quot;button[title=&apos;Show API Key&apos;]&quot;))

    logInfo(&apos;Extracting API Key&apos;)
    String apiKey = getVisibleText(By.tagName(&quot;code&quot;))

    if (!apiKey) {
        throw new IllegalStateException(&quot;API key element located but text was empty&quot;)
    }

    props.put(&apos;apiKey&apos;, apiKey)
    logInfo(&quot;API Key captured and stored in props[&apos;apiKey&apos;]&quot;)

} catch (Throwable t) {
    log.error(&apos;Failed to obtain API key&apos;, t)
    throw t
} finally {
    driver.quit()
    logInfo(&apos;Browser closed&apos;)
}
